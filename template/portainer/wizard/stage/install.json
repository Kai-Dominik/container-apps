{
  "api_version": "v1",
  "title": "{{PORTAINER_NAME}}",
  "wizard": [
    {
      "title": "{{PORTAINER_CONFIGURE}}",
      "schema": {
        "http_port": {
          "type": "integer",
          "title": "HTTP Port",
          "description": "{{PORTAINER_WEB_HOST_PORT_DESC}}",
		      "required": true
        },
        "container_name": {
          "type": "string",
          "title": "Containner Name",
          "description": "{{PORTAINER_NAME_DESC}}",
		      "required": true
        }
      },
      "form": [
        "*"
      ]
    }
  ],
  "binding": {
    "type": "yaml",
    "file": "docker-compose.yml",
    "data": {
      "http_port": [
        "portainer.ports[0]"
      ],
      "container_name": "container_name"
    }
  }
}

        